<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias type="com.moviehub.biz.movie.MovieVO" alias="movie"/>
		<typeAlias type="com.moviehub.biz.user.UserVO" alias="user"/>
	</typeAliases>

	<!-- Sql Mapper 설정  -->
	<mappers>
		<mapper resource="mappings/user-mapping.xml" />
		<mapper resource="mappings/movie-mapping.xml" />
	</mappers>
</configuration>